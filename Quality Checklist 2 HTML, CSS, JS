    <script>
    function updateContent() {
        document.getElementById("content").style.display = "none";
        document.getElementById("bottomButton").style.display = "block";

        let material = prompt("Material is Astron - Yes or No?");
        lowerMaterial = material.toLowerCase();
        if (lowerMaterial === "yes") {
            document.getElementById("astronSpecifics").style.display = "block";
        } else {
            document.getElementById("astronSpecifics").style.display = "none";
        }
        
        let surface = prompt("Occlusal Surface Type?");
        lowerSurface = surface.toLowerCase();
        if (lowerSurface === "flat plane") {
            document.getElementById("flatPlaneContent").style.display = "block";
        } else if (lowerSurface === "follow anatomy") {
            document.getElementById("followAnatomyContent").style.display = "block";
        } else {
            document.getElementById("posteriorIndentationsContent").style.display = "block";
        }
        
        let canineGuidance = prompt("Needs Canine Guidance - Yes or No?");
        lowerCanineGuidance = canineGuidance.toLowerCase();
        if (lowerCanineGuidance === "yes") {
            document.getElementById("canineGuidanceSpecifics").style.display = "block";
        } else {
            document.getElementById("canineGuidanceSpecifics").style.display = "none";
        }

        let anteriorGuidance = prompt("Needs Anterior Guidance - Yes or No?");
        lowerAnteriorGuidance = anteriorGuidance.toLowerCase();
        if (lowerAnteriorGuidance === "yes") {
            document.getElementById("anteriorGuidanceSpecifics").style.display = "block";
        } else {
            document.getElementById("anteriorGuidanceSpecifics").style.display = "none";
        }
    }

    let materialButton = document.querySelector("button");
    materialButton.addEventListener("click", updateContent);
    </script>
</body>
</html>
