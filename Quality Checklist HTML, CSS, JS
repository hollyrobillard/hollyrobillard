    <script>
    function updateContent() {
        let material = prompt("Material Type?");
        lowerMaterial = material.toLowerCase();
        if (lowerMaterial === "acetal") {
            document.getElementById("acetalContent").style.display = "block";
            document.getElementById("content").style.display = "none";
            document.getElementById("flexibleContent").style.display = "none";
            document.getElementById("metalContent").style.display = "none";
        } else if (lowerMaterial === "valplast" || lowerMaterial === "tcs" || lowerMaterial === "flexible") {
            document.getElementById("flexibleContent").style.display = "block";
            document.getElementById("content").style.display = "none";
            document.getElementById("metalContent").style.display = "none";
            document.getElementById("acetalContent").style.display = "none";
        } else if (lowerMaterial === "metal" || lowerMaterial === "titanium" || lowerMaterial === "cc" || lowerMaterial === "chromecobalt" || lowerMaterial === "chrome cobalt" || lowerMaterial === "chrome-cobalt") {
            document.getElementById("metalContent").style.display = "block";
            document.getElementById("content").style.display = "none";
            document.getElementById("flexibleContent").style.display = "none";
            document.getElementById("acetalContent").style.display = "none";
        } else {
            document.getElementById("content").style.display = "block";
            document.getElementById("flexibleContent").style.display = "none";
            document.getElementById("acetalContent").style.display = "none";
            document.getElementById("metalContent").style.display = "none";
        }
    }

    let materialButton = document.querySelector("button");
    materialButton.addEventListener("click", updateContent);
    </script>
</body>
</html>
